Dialogue: 0,0:22:30.12,0:22:31.78,SIGN,,0000,0000,0000,,{\pos(320,263)}Preview
Dialogue: 0,0:22:30.98,0:22:34.12,Default,,0000,0000,0000,,"Those whose names are written in this notebook will die."
Dialogue: 0,0:22:34.12,0:22:37.84,Default,,0000,0000,0000,,"This note will not take effect unless the writer has the person's face in their mind when they write his/her name."
Dialogue: 0,0:22:37.84,0:22:41.92,Default,,0000,0000,0000,,"Therefore, people sharing the same name will not be affected."
